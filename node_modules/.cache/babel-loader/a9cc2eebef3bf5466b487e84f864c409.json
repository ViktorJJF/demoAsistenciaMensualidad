" class=\"mb-3\" placeholder=\"Your Post Title\" />\n        <div ref=\"editor\" class=\"add-new-post__editor mb-1\"></div>\n      </d-form>\n    </d-card-body>\n  </d-card>\n</template>\n\n<script>\nimport Quill from 'quill';\n\nexport default {\n  name: 'editor',\n  mounted() {\n    const toolbarOptions = [\n      [{ header: [1, 2, 3, 4, 5, false] }],\n      ['bold', 'italic', 'underline', 'strike'],\n      ['blockquote', 'code-block'],\n      [{ header: 1 }, { header: 2 }],\n      [{ list: 'ordered' }, { list: 'bullet' }],\n      [{ script: 'sub' }, { script: 'super' }],\n      [{ indent: '-1' }, { indent: '+1' }],\n    ];\n\n    // Init the Quill RTE\n    new Quill(this.$refs.editor, {\n      modules: {\n        toolbar: toolbarOptions,\n      },\n      placeholder: 'Words can be like x-rays if you use them properly...',\n      theme: 'snow',\n    });\n  },\n};\n</script>\n\n"],"sourceRoot":"src/components/add-new-post"}]}