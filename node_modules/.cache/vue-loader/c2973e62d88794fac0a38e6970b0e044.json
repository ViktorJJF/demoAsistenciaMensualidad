ass=\"border-top\">\n      <d-row>\n\n        <!-- Time Frame -->\n        <d-col>\n          <d-select size=\"sm\" value=\"last-week\" style=\"max-width: 130px;\">\n            <option value=\"last-week\">Last Week</option>\n            <option value=\"today\">Today</option>\n            <option value=\"last-month\">Last Month</option>\n            <option value=\"last-year\">Last Year</option>\n          </d-select>\n        </d-col>\n\n        <!-- View Full Report -->\n        <d-col class=\"text-right view-report\">\n          <a href=\"#\">View full report &rarr;</a>\n        </d-col>\n\n      </d-row>\n    </d-card-footer>\n\n  </d-card>\n</template>\n\n<script>\nconst defaultTopReferrals = [{\n  title: 'GitHub',\n  value: '19,291',\n}, {\n  title: 'Stack Overflow',\n  value: '11,201',\n}, {\n  title: 'Hacker News',\n  value: '9,291',\n}, {\n  title: 'Reddit',\n  value: '8,281',\n}, {\n  title: 'The Next Web',\n  value: '7,128',\n}, {\n  title: 'Tech Crunch',\n  value: '6,218',\n}, {\n  title: 'YouTube',\n  value: '1,218',\n}, {\n  title: 'Adobe',\n  value: '1,171',\n},\n];\n\nexport default {\n  name: 'ao-top-referrals',\n  props: {\n    /**\n       * The component's title.\n       */\n    title: {\n      type: String,\n      default: 'Top Referrals',\n    },\n    /**\n       * The referral datasets.\n       */\n    referralData: {\n      type: Array,\n      default() {\n        return defaultTopReferrals;\n      },\n    },\n  },\n};\n</script>\n"]}]}